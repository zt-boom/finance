{
  "manifest_version": 3,
  "name": "基金每日预估收益",
  "description": "记录基金持仓，获取预估涨跌并计算每日预估收益。",
  "version": "1.0.0",
  "author": "Your Name",
  "homepage_url": "https://github.com/yourusername/fund-estimator",
  "minimum_chrome_version": "88",
  "offline_enabled": true,
  "action": {
    "default_title": "基金每日预估收益 v1.0",
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "https://fundgz.1234567.com.cn/*",
    "https://fund.eastmoney.com/*"
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}
